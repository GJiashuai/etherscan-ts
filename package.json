    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.4",
    "ts-node-dev": "^1.1.6",
    "tsutils": "^3.21.0",
    "typescript": "^4.2.4"
  },
  "scripts": {
    "dev": "yarn prettier && ts-node-dev src/main.ts --trace-sync-io",
